#include <stdio.h>
void Swap(int*A ,int *B);
int main(){
    int X,Y;
  printf("please,enter first number");
  scanf("%d",&X);  
   printf("please,enter second number");
  scanf("%d",&Y);
  Swap(&X ,&Y);
  }

void Swap(int*A ,int *B){
    int temp=*A;
   * A=*B;
    *B=temp;
    printf("After swapping\n");
    printf("A = %d\n",*A);
    printf("B = %d\n",*B);
}
